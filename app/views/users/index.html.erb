<h1>USER</h1>

<table>
	<tr>
		<th>Username</th>
		<th>Email</th>
		<th>Role</th>
		<th>Password</th>
		<!-- <th colspan='2'></th> -->
	</tr>

<% @users.each do |user| %>
	<tr>
		<td><%= user.username %></td>
		<td><%= user.email %></td>
		<td><%= user.role.name %></td>
		<td><%= user.encrypted_password %></td>
	</tr>
<% end %>
</table>